{
    "name": "dcolsay/laravel-drive",
    "description": "Filemanager",
    "authors": [
        {
            "name": "Boris DEHOUMON",
            "email": "borisdehoumon@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.6",
        "illuminate/contracts": "^8.0",
        "filament/filament": "^1.9",
        "spatie/laravel-medialibrary": "^9.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Dcolsay\\Drive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dcolsay\\Drive\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcolsay\\Drive\\DriveServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
